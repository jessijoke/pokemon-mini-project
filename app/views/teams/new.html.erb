<div class="page_title">
    <h1 class="h1_title">
        New Team
    </h1>
</div>

<div class="page_content">

    <%= form_tag teams_path do %>
    <label>Team Name:</label><br>
    <%= text_field_tag :name %><br>

    <label>Trainer ID:</label><br>
    <%= number_field_tag :trainer_id %><br>

    <%= submit_tag "Add Team" %>
    <% end %>

 </div>
